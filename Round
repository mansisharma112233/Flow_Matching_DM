import sys

data = sys.stdin.read().split()
idx = 0

def check(x):
    d = 0
    z = 0
    while x:
        if x % 10 == 0:
            z += 1
        d += 1
        x //= 10
    return z == d - 1

round_nums = []
for i in range(1, 1000000):
    if check(i):
        round_nums.append(i)

t = int(data[idx])
idx += 1

for _ in range(t):
    n = int(data[idx])
    idx += 1

    cnt = 0
    for v in round_nums:
        if v <= n:
            cnt += 1
        else:
            break
    print(cnt)
